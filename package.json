{
  "name": "hue-datacenter-visualization",
  "version": "2.05.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "dev:turbo": "next dev --turbopack",
    "build": "NEXT_PUBLIC_SKIP_TYPE_CHECK=true next build",
    "build:with-types": "next build",
    "start": "next start",
    "lint": "next lint",
    "type-check": "tsc --noEmit",
    "prepare": "husky || true",
    "format": "prettier --write .",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "playwright test --config=playwright.coverage.config.ts",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:debug": "playwright test --debug",
    "test:playwright": "playwright test --config=playwright.comprehensive.config.ts",
    "test:playwright:ui": "playwright test --config=playwright.comprehensive.config.ts --ui",
    "test:playwright:headed": "playwright test --config=playwright.comprehensive.config.ts --headed",
    "test:playwright:debug": "playwright test --config=playwright.comprehensive.config.ts --debug",
    "test:unit:playwright": "playwright test --config=playwright.comprehensive.config.ts --project=unit-tests",
    "test:integration:playwright": "playwright test --config=playwright.comprehensive.config.ts --project=integration-tests",
    "test:e2e:comprehensive": "playwright test --config=playwright.comprehensive.config.ts --project=e2e-*",
    "test:accessibility": "playwright test --config=playwright.comprehensive.config.ts --project=accessibility-tests",
    "test:performance": "playwright test --config=playwright.comprehensive.config.ts --project=performance-tests",
    "test:responsive": "playwright test --config=playwright.comprehensive.config.ts --project=responsive-*",
    "test:visual": "playwright test --config=playwright.comprehensive.config.ts --project=visual-regression",
    "test:mobile": "playwright test --config=playwright.comprehensive.config.ts --project=*mobile*",
    "test:desktop": "playwright test --config=playwright.comprehensive.config.ts --project=*desktop*",
    "test:browsers": "playwright test --config=playwright.comprehensive.config.ts --project=e2e-chromium --project=e2e-firefox --project=e2e-webkit",
    "test:coverage:report": "node -e \"require('./tests/config/coverage.config.ts').generateCoverageReport()\"",
    "test:coverage:unit": "playwright test --config=playwright.coverage.config.ts --project=coverage-unit",
    "test:coverage:integration": "playwright test --config=playwright.coverage.config.ts --project=coverage-integration",
    "test:coverage:e2e": "playwright test --config=playwright.coverage.config.ts --project=coverage-e2e",
    "test:coverage:full": "npm run test:coverage && npm run test:coverage:report",
    "test:redundancy": "jest --testPathPatterns=redundancy",
    "test:redundancy:watch": "jest --testPathPatterns=redundancy --watch",
    "test:redundancy:coverage": "jest --testPathPatterns=redundancy --coverage",
    "test:redundancy:e2e": "playwright test --config=playwright.comprehensive.config.ts tests/e2e/RedundancyVisualization.e2e.spec.ts",
    "test:redundancy:all": "npm run test:redundancy && npm run test:redundancy:e2e",
    "test:ci": "npm run test:coverage:full && npm run test:playwright",
    "test:ci:fast": "npm run test && npm run test:e2e:comprehensive",
    "test:ci:full": "npm run test:coverage:full && npm run test:playwright && npm run test:accessibility && npm run test:performance",
    "test:local": "npm run test:watch",
    "test:local:e2e": "npm run test:e2e:ui",
    "test:local:full": "npm run test && npm run test:e2e",
    "test:all": "npm run test:ci:full",
    "test:quick": "npm run test && npm run test:e2e:comprehensive",
    "test:smoke": "playwright test --config=playwright.comprehensive.config.ts tests/e2e/RedundancyVisualization.e2e.spec.ts --project=e2e-chromium",
    "test:install": "playwright install",
    "test:install:deps": "playwright install-deps",
    "test:doctor": "playwright doctor",
    "test:show-report": "playwright show-report",
    "test:show-coverage": "open coverage/html/index.html",
    "test:unit": "jest --testPathPattern=unit",
    "test:integration": "jest --testPathPattern=integration",
    "test:security": "npm audit && snyk test",
    "test:build": "npm run build && npm run test:smoke",
    "dev:redundancy": "NEXT_PUBLIC_ENABLE_REDUNDANCY=true npm run dev",
    "dev:test": "NODE_ENV=test npm run dev",
    "type-check:redundancy": "cd features/redundancy && tsc --noEmit",
    "lint:redundancy": "eslint features/redundancy --ext .ts,.tsx",
    "build:redundancy": "cd features/redundancy && npm run build"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write",
      "npm run type-check"
    ],
    "*.{json,md,yml,yaml}": [
      "prettier --write"
    ],
    "*.{css,scss}": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "@radix-ui/react-dialog": "^1.1.14",
    "@radix-ui/react-hover-card": "^1.1.14",
    "@radix-ui/react-select": "^2.2.5",
    "@radix-ui/react-toggle": "^1.1.9",
    "@radix-ui/react-tooltip": "^1.2.7",
    "@tanstack/react-query": "^5.81.5",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "autoprefixer": "^10.4.21",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "framer-motion": "^12.23.0",
    "lucide-react": "^0.525.0",
    "next": "15.3.5",
    "postcss": "^8.4.47",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "sharp": "^0.34.2",
    "tailwind-merge": "^2.5.4",
    "tailwindcss": "^3.4.15",
    "tailwindcss-animate": "^1.0.7",
    "typescript": "^5",
    "web-vitals": "^5.0.3",
    "zustand": "^5.0.6"
  },
  "devDependencies": {
    "@axe-core/playwright": "^4.10.2",
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@eslint/eslintrc": "^3",
    "@next/bundle-analyzer": "^15.3.5",
    "@playwright/test": "^1.53.2",
    "@tanstack/react-query-devtools": "^5.81.5",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest": "^30.0.0",
    "@typescript-eslint/eslint-plugin": "^8.35.1",
    "@typescript-eslint/parser": "^8.35.1",
    "chrome-launcher": "^1.2.0",
    "eslint": "^9",
    "eslint-config-next": "15.3.5",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-prettier": "^5.5.1",
    "eslint-plugin-react-hooks": "^5.2.0",
    "husky": "^9.1.7",
    "jest": "^30.0.4",
    "jest-environment-jsdom": "^30.0.4",
    "lighthouse": "^12.7.1",
    "lighthouse-ci": "^1.10.0",
    "lint-staged": "^16.1.2",
    "pa11y-ci": "^2.4.2",
    "prettier": "^3.6.2",
    "snyk": "^1.1297.3",
    "source-map-explorer": "^2.5.3",
    "webpack-bundle-analyzer": "^4.10.2"
  }
}
